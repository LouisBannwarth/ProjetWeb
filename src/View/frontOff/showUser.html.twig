{% extends "layout.html.twig" %}



{% block contenu %}
    <div class="row">
        <table>
            <caption>Recapitulatifs utilisateur</caption>
            <thead>
            <tr><th>nom</th><th>email</th><th>login</th><th>code postal</th><th>ville</th><th>adresse</th>
                {% if   app.session.get('droit') is defined and  app.session.get('droit') == 'DROITclient' %}
                    <th>op√©ration</th>
                {% endif %}
            </tr>
            </thead>
            <tbody>
            {% for produit in data if data is not empty %}

                <tr>
                    <td>{{produit.nom}}</td><td>{{produit.email}}</td><td>{{produit.login}}</td><td>{{produit.code_postal}}</td><td>
                        {{produit.ville}}</td><td>{{ produit.adresse }}</td>

                </tr>
            {% endfor %}
            <tbody>
        </table>
    </div>

{% endblock %}